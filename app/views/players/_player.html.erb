<div id="<%= dom_id player %>">
  <p>
    <strong>Full name:</strong>
    <%= player.full_name %>
  </p>

 <p>
    <strong>Primary Position:</strong>
     <%= player.primary_position.try(:humanize) %>
  </p>


  <p>
    <strong>Age:</strong>
    <%= player.age %>
  </p>

  <p>
    <strong>Grade:</strong>
    <%= player.grade %>
  </p>

  <p>
    <strong>School:</strong>
    <%= player.school %>
  </p>

  <p>
    <strong>Notes:</strong>
    <%= player.notes %>
  </p>

</div>
